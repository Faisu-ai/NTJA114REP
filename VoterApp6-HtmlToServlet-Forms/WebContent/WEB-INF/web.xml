<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xmlns="http://xmlns.jcp.org/xml/ns/javaee"
                  xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                    http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">

   <welcome-file-list>
     <welcome-file>form.html</welcome-file>
      <welcome-file>homeurl</welcome-file>
      <welcome-file>form2.html</welcome-file>
      <welcome-file>home.jsp</welcome-file>
  </welcome-file-list> 

  <servlet>
    <servlet-name>voter</servlet-name>
    <servlet-class>com.nt.servlet.VoterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>voter</servlet-name>
      <url-pattern>/voterurl</url-pattern>
  </servlet-mapping>
  
   <servlet>
    <servlet-name>welcome</servlet-name>
    <servlet-class>com.nt.servlet.HomePageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>welcome</servlet-name>
      <url-pattern>/homeurl</url-pattern>
  </servlet-mapping>

</web-app>